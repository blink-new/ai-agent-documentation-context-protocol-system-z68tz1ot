# ===== DEPLOYMENT =====
# Production deployment settings
DEPLOYMENT_URL=https://ai-agent-documentation-context-protocol-system-z68tz1ot.sites.blink.new
API_BASE_URL=https://z68tz1ot--agent-api.functions.blink.new
WEBSOCKET_URL=wss://realtime.blink.new

# CDN and caching
CDN_URL=your_cdn_url_here
CACHE_TTL=3600

# ===== FEATURE FLAGS =====
# Enable/disable specific features
ENABLE_VECTOR_SEARCH=true
ENABLE_REAL_TIME_PROCESSING=true
ENABLE_PLUGIN_SYSTEM=true
ENABLE_ANALYTICS=true
ENABLE_WEBHOOKS=true
ENABLE_COLLABORATION=true

# ===== PERFORMANCE TUNING =====
# Performance and scaling settings
MAX_DOCUMENT_SIZE=50MB
MAX_CONCURRENT_PROCESSES=10
VECTOR_DIMENSION=1536
CHUNK_SIZE=1000
CHUNK_OVERLAP=200
