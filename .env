# ===== COMPLIANCE & PRIVACY =====
# Data privacy and compliance
GDPR_COMPLIANCE=true
DATA_RETENTION_DAYS=365
ENCRYPTION_KEY=your_encryption_key_here
AUDIT_LOG_ENABLED=true

# ===== CUSTOM INTEGRATIONS =====
# Custom webhook endpoints for specific integrations
CUSTOM_WEBHOOK_1=your_custom_webhook_1_here
CUSTOM_WEBHOOK_2=your_custom_webhook_2_here

# Custom API endpoints
CUSTOM_API_ENDPOINT_1=your_custom_api_1_here
CUSTOM_API_ENDPOINT_2=your_custom_api_2_here

