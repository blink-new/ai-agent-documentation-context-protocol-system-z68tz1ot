# Rate limiting
RATE_LIMIT_REQUESTS_PER_MINUTE=100
RATE_LIMIT_REQUESTS_PER_HOUR=1000
RATE_LIMIT_REQUESTS_PER_DAY=10000

# ===== BACKUP & RECOVERY =====
# Backup configurations
BACKUP_SCHEDULE=0 2 * * *
BACKUP_RETENTION_DAYS=30
